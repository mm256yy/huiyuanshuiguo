.menu_box {
  width: 100%;
  margin: 0 auto;
  background-color: #fff;
  border-radius: 20rpx;
  margin-bottom: 30rpx;
}
.menu_box .head {
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 100rpx;
  width: 90%;
  margin: 0 auto;
}
.menu_box .head .left {
  color: #1c1c1c;
  font-size: 30rpx;
  font-weight: bold;
  line-height: 30rpx;
  display: flex;
  align-items: center;
}
.menu_box .head .right {
  color: #999;
  display: flex;
  align-items: center;
  font-size: 26rpx;
  line-height: 26rpx;
}
.menu_box .delivery_box {
  width: 90%;
  margin: 0 auto;
  padding: 10rpx 0 50rpx;
}
.menu_box .delivery_box .u-radio {
  flex: 1;
}
.menu_box .phone_box {
  width: 90%;
  margin: 0 auto;
  padding: 10rpx 0 50rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  position: relative;
}
.menu_box .phone_box .input {
  flex: 1;
  height: 70rpx;
  border: 1px solid #eee;
  border-radius: 10rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.menu_box .phone_box .input input {
  width: 92%;
  color: #1c1c1c;
  font-size: 30rpx;
  height: 100%;
}
.menu_box .phone_box .btn {
  color: #F54319;
  border: 1px solid #F54319;
  height: 70rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 0 20rpx;
  font-size: 28rpx;
  border-radius: 10rpx;
  margin-left: 20rpx;
  position: relative;
}
.menu_box .phone_box .btn button {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  opacity: 0;
}
.menu_box .address_box {
  width: 90%;
  margin: 0 auto;
  padding-bottom: 30rpx;
}
.menu_box .address_box .address {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.menu_box .address_box .address .left {
  flex: 1;
  min-width: 0;
}
.menu_box .address_box .address .left .tit {
  color: #1c1c1c;
  font-size: 32rpx;
}
.menu_box .address_box .address .left .tit ._span {
  color: #666;
  padding-left: 20rpx;
  font-size: 28rpx;
}
.menu_box .address_box .address .left .txt {
  font-size: 28rpx;
  color: #666;
  padding-top: 10rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.menu_box .address_box .address .right {
  width: 80rpx;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
.menu_box .address_box .none_address {
  width: 100%;
  height: 150rpx;
  border: 1px dashed #e5e5e5;
  border-radius: 10rpx;
  color: #bbb;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.menu_box .address_box .none_address ._span {
  font-size: 26rpx;
}
.menu_box .goods_box {
  padding-bottom: 2rpx;
}
.menu_box .goods_box .goods {
  width: 90%;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 40rpx;
}
.menu_box .goods_box .goods .left {
  width: 200rpx;
}
.menu_box .goods_box .goods .left image {
  width: 180rpx;
  height: 180rpx;
  border-radius: 20rpx;
}
.menu_box .goods_box .goods .right {
  flex: 1;
  min-width: 0;
  height: 170rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.menu_box .goods_box .goods .right .title {
  color: #1c1c1c;
  font-size: 30rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.menu_box .goods_box .goods .right .text {
  color: #999;
  font-size: 28rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.menu_box .goods_box .goods .right .price {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.menu_box .goods_box .goods .right .price .num {
  color: #f14b5b;
  font-size: 34rpx;
  font-weight: bold;
  display: flex;
  align-items: center;
}
.menu_box .goods_box .goods .right .price .num ._span {
  font-size: 28rpx;
  padding-left: 2rpx;
}
.menu_box .goods_box .goods .right .price .buy {
  color: #F54319;
  padding: 0 10rpx;
  border-radius: 10rpx;
  font-size: 28rpx;
  font-weight: bold;
}
.menu_box .coupons_box {
  padding: 0 0 20rpx;
}
.menu_box .coupons_box .label {
  width: 90%;
  height: 120rpx;
  margin: 0 auto;
  border: 1px solid #f5f5f5;
  border-radius: 20rpx;
  display: flex;
  justify-content: space-between;
  margin-bottom: 20rpx;
}
.menu_box .coupons_box .label .price {
  width: 150rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #f14b5b;
  font-size: 38rpx;
  font-weight: bold;
  line-height: 38rpx;
}
.menu_box .coupons_box .label .price ._span {
  font-size: 24rpx;
  padding-left: 2rpx;
  margin-bottom: -4rpx;
}
.menu_box .coupons_box .label .info {
  flex: 1;
  min-width: 0;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.menu_box .coupons_box .label .info .title {
  color: #1c1c1c;
  font-size: 30rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.menu_box .coupons_box .label .info .text {
  font-size: 26rpx;
  color: #999;
  padding-top: 4rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.menu_box .coupons_box .label .button {
  width: 110rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.menu_box .order_info_box .order_head {
  width: 90%;
  margin: 0 auto;
}
.menu_box .order_info_box .order_head .label {
  display: flex;
  align-items: center;
  padding-bottom: 30rpx;
  justify-content: space-between;
  font-size: 28rpx;
  color: #999;
}
.menu_box .order_info_box .order_foot {
  border-top: 1px solid #f5f5f5;
}
.menu_box .order_info_box .order_foot .label {
  display: flex;
  align-items: center;
  width: 90%;
  margin: 0 auto;
  justify-content: space-between;
  height: 100rpx;
  font-weight: bold;
  font-size: 30rpx;
}
.pay_type_box {
  width: 92%;
  margin: 0 auto;
  background-color: #fff;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-radius: 20rpx;
  height: 100rpx;
}
.pay_type_box .icons {
  width: 100rpx;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.pay_type_box .title {
  flex: 1;
  color: #1c1c1c;
  font-size: 28rpx;
}
.pay_type_box .right {
  width: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100%;
}
.footer {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  z-index: 100;
  background-color: #fff;
  padding-bottom: env(safe-area-inset-bottom);
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
}
.footer .label {
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 120rpx;
  width: 100%;
}
.footer .label .button {
  width: 92%;
  margin: 0 auto;
  height: 80rpx;
  border-radius: 100rpx;
  color: #fff;
  font-size: 32rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.footer .label .button .u-count-down__text {
  color: #fff;
  line-height: 30rpx;
  padding: 0 4rpx;
}

