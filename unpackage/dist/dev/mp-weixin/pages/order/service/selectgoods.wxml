<block wx:if="{{orderDetail}}"><view class="content"><view class="orderBox"><view class="order"><view class="label"><view class="title">订单编号</view><view class="number">{{orderDetail.order_number}}</view></view><view class="label"><view class="title">下单时间</view><view class="number">{{orderDetail.create_time}}</view></view><block wx:if="{{orderDetail.paypal_time}}"><view class="label"><view class="title">支付时间</view><view class="number">{{orderDetail.paypal_time}}</view></view></block><block wx:if="{{orderDetail.paypal_type}}"><view class="label"><view class="title">支付方式</view><view class="number"><block wx:if="{{orderDetail.paypal_type==1}}"><text>微信支付</text></block><block wx:if="{{orderDetail.paypal_type==2}}"><text>货到付款</text></block><block wx:if="{{orderDetail.paypal_type==3}}"><text>余额支付</text></block><block wx:if="{{orderDetail.paypal_type==4}}"><text>线下支付</text></block></view></view></block><view class="label"><view class="title">实际支付</view><view class="number">{{"¥"+orderDetail.total_money}}</view></view><block wx:if="{{orderType=='tk'}}"><view class="label"><view class="title">退款状态</view><view class="number">{{orderDetail.return_status?'已退 ¥'+orderDetail.return_money:'未申请过退款'}}</view></view></block><block wx:if="{{orderType=='sh'}}"><view class="label"><view class="title">售后状态</view><view class="number">{{orderDetail.service_status?'已申请过售后':'未申请过售后'}}</view></view></block></view></view><view class="goodsBox"><block wx:for="{{orderDetail.goods_list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['selectGoodsTap',[index]]]]]}}" class="goods box_shadow" bindtap="__e"><view class="icons"><block wx:if="{{item.active}}"><aicon vue-id="{{'99412ba0-1-'+index}}" type="checkbox-circle-fill" color="#F54319" size="25" bind:__l="__l"></aicon></block><block wx:else><aicon vue-id="{{'99412ba0-2-'+index}}" type="checkbox-blank-circle-line" color="#eee" size="25" bind:__l="__l"></aicon></block></view><view class="image"><image src="