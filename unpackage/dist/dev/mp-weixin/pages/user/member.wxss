.headerBox {
  width: 100%;
  height: 350rpx;
  background-color: #f5f5f5;
  display: flex;
  align-items: flex-end;
  justify-content: center;
}
.headerBox .vipBox {
  width: 90%;
  height: 310rpx;
  background: linear-gradient(-45deg, #c68c50, #f8d4ad);
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  position: relative;
}
.headerBox .vipBox .user {
  display: flex;
  align-items: center;
  width: 90%;
  margin: 0 auto;
  margin-top: 20rpx;
  justify-content: space-between;
}
.headerBox .vipBox .user .avatar {
  height: 100rpx;
  width: 100rpx;
  border-radius: 100rpx;
}
.headerBox .vipBox .user .nameBox {
  width: 77%;
  min-width: 0;
}
.headerBox .vipBox .user .nameBox .nickname {
  color: #fff;
  font-weight: bold;
  font-size: 30rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.headerBox .vipBox .user .nameBox .totle {
  color: #fff;
  font-size: 24rpx;
  padding-top: 6rpx;
}
.headerBox .vipBox .tips {
  width: 90%;
  padding-bottom: 30rpx;
}
.headerBox .vipBox .tips .title {
  color: #fff;
  font-size: 30rpx;
  margin-bottom: -8rpx;
}
.headerBox .vipBox .tips .text {
  color: #fff;
  font-size: 26rpx;
  display: flex;
  justify-content: space-between;
}
.headerBox .vipBox .noneTips {
  height: 180rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  font-size: 60rpx;
  color: #f8d4ad;
  padding-bottom: 20rpx;
}
.headerBox .vipBox .bgImg {
  position: absolute;
  top: 50rpx;
  right: 0;
  opacity: 0.2;
}
.contentBox {
  box-shadow: 0 -30rpx 30rpx #00000010;
  position: absolute;
  width: 100%;
  padding-top: 20rpx;
}
.contentBox .bigBox {
  width: 90%;
  margin: 0 auto;
  padding-bottom: 50rpx;
}
.contentBox .bigBox .header {
  display: flex;
  align-items: center;
  font-size: 34rpx;
  font-weight: bold;
  height: 80rpx;
}
.contentBox .bigBox .hover {
  display: flex;
  align-items: center;
  justify-content: space-between;
  flex-wrap: wrap;
  padding-top: 20rpx;
}
.contentBox .bigBox .hover .label {
  background-color: #f5f5f5;
  flex: 0 0 30%;
  height: 200rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  border-radius: 10rpx;
  border: 1px solid #f5f5f5;
  margin-bottom: 30rpx;
}
.contentBox .bigBox .hover .label .title {
  color: #c68c50;
  font-weight: bold;
  font-size: 26rpx;
}
.contentBox .bigBox .hover .label .text {
  color: #c68c50;
  font-size: 22rpx;
  padding-top: 10rpx;
}
.contentBox .bigBox .hover .label .price {
  color: #c68c50;
  font-weight: bold;
  font-size: 46rpx;
  height: 80rpx;
  display: flex;
  align-items: center;
}
.contentBox .bigBox .hover .label .price ._span {
  font-size: 30rpx;
  margin-bottom: -8rpx;
  margin-right: 4rpx;
}
.contentBox .bigBox .hover .active {
  border-color: #c68c5070;
  background-color: #c68c5010;
}
.contentBox .bigBox .button {
  background: #ffdfb2;
  width: 100%;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 100rpx;
  margin-top: 30rpx;
  color: #c68c50;
  font-weight: bold;
  font-size: 28rpx;
}
.contentBox .bigBox .none {
  background-color: #eee;
  color: #999;
}
.contentBox .interestsBox {
  width: 90%;
  margin: 0 auto;
  padding-bottom: 50rpx;
}
.contentBox .interestsBox .header {
  display: flex;
  align-items: center;
  font-size: 34rpx;
  font-weight: bold;
  height: 80rpx;
}
.contentBox .interestsBox .listBox {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.contentBox .interestsBox .listBox .list {
  flex: 0 0 23%;
  margin-bottom: 50rpx;
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: center;
  min-width: 0;
}
.contentBox .interestsBox .listBox .list .image {
  background-color: #c68c5010;
  border-radius: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 120rpx;
  height: 120rpx;
}
.contentBox .interestsBox .listBox .list .image image {
  width: 80rpx;
  height: 80rpx;
}
.contentBox .interestsBox .listBox .list .title {
  padding-top: 10rpx;
  font-size: 26rpx;
  text-align: center;
  font-weight: bold;
  width: 95%;
  margin: 0 auto;
}
.contentBox .interestsBox .listBox .list .text {
  color: #999;
  font-size: 24rpx;
  text-align: center;
  width: 95%;
  margin: 0 auto;
  padding-top: 10rpx;
}
.headerBoxa {
  width: 90%;
  margin: 0 auto;
  height: 320rpx;
  margin-top: 30rpx;
  box-shadow: 0 6rpx 20rpx rgba(0, 0, 0, 0.08);
  position: relative;
  overflow: hidden;
  font-weight: normal;
  border-radius: 6rpx;
}
.headerBoxa .bgImg {
  position: absolute;
  top: 0;
  right: -10rpx;
  width: 300rpx;
  height: 320rpx;
  opacity: 0.05;
  display: flex;
  align-items: center;
  justify-content: center;
}
.headerBoxa .user {
  display: flex;
  width: 90%;
  margin: 0 auto;
  height: 120rpx;
  align-items: flex-end;
}
.headerBoxa .user .avatar {
  width: 90rpx;
  height: 90rpx;
  display: block;
  border-radius: 100%;
  overflow: hidden;
}
.headerBoxa .user .nameBox {
  color: #fff;
  width: 80%;
  height: 96rpx;
  padding-left: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-start;
}
.headerBoxa .user .nameBox .nickname {
  font-size: 28rpx;
}
.headerBoxa .user .nameBox .totle {
  font-size: 24rpx;
  padding-top: 4rpx;
}
.headerBoxa .status {
  width: 100%;
  height: 200rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color: #fff;
}
.headerBoxa .status .name {
  font-size: 46rpx;
}
.headerBoxa .status .text {
  font-size: 26rpx;
  padding-top: 12rpx;
}
.agentListBox {
  width: 90%;
  margin: 0 auto;
  margin-top: 30rpx;
  font-weight: normal;
}
.agentListBox .header {
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 34rpx;
  font-weight: bold;
  height: 80rpx;
  margin-bottom: 20rpx;
}
.agentListBox .menuBox {
  display: flex;
  width: 100%;
  height: 80rpx;
  align-items: center;
  background: linear-gradient(-45deg, #c68c50, #f8d4ad);
}
.agentListBox .menuBox .menu {
  flex: 0 0 33.33%;
  color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 80rpx;
}
.agentListBox .menuBox .menu:nth-child(1),
.agentListBox .menuBox .menu:nth-child(2) {
  border-right: 2rpx solid #f8d4ad50;
}
.agentListBox .listBox {
  display: flex;
  width: 100%;
  height: 80rpx;
  align-items: center;
}
.agentListBox .listBox .list {
  flex: 0 0 33.33%;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 80rpx;
  font-size: 26rpx;
}
.agentListBox .listBox .list:nth-child(1),
.agentListBox .listBox .list:nth-child(2) {
  border-right: 2rpx solid #f8d4ad50;
}
.agentListBox .listBox:nth-child(odd) {
  border-bottom: 2rpx solid #f8d4ad50;
  border-left: 2rpx solid #f8d4ad50;
  border-right: 2rpx solid #f8d4ad50;
}
.agentListBox .listBox:nth-child(even) {
  border-left: 2rpx solid #f8d4ad50;
  border-right: 2rpx solid #f8d4ad50;
  background-color: #f8d4ad50;
}
.agentListBox .text {
  display: flex;
  height: 80rpx;
  align-items: center;
  justify-content: center;
  color: #999;
  font-size: 24rpx;
}

