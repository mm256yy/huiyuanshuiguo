page {
  background-color: #fff;
}
.swiper_box {
  width: 100%;
  position: relative;
  background-color: #ffffff;
}
.swiper_box swiper {
  width: 90%;
  height: 200rpx;
  margin: 0 auto;
  border-radius: 20rpx;
  overflow: hidden;
  padding: 10rpx 0 30rpx;
}
.swiper_box swiper image {
  width: 100%;
  height: 200rpx;
  margin: 0 auto;
  border-radius: 20rpx;
}
.swiper_box .swiper_dots {
  position: absolute;
  bottom: 50rpx;
  display: flex;
  justify-content: center;
  width: 92%;
  z-index: 6;
  left: 0;
  right: 0;
  margin: 0 auto;
}
.swiper_box .swiper_dots .dot {
  margin: 0 4rpx;
  width: 10rpx;
  height: 10rpx;
  background: #fff;
  border-radius: 20rpx;
  transition: all 0.1s;
  opacity: 0.5;
}
.swiper_box .swiper_dots .active {
  opacity: 1;
  background: #da0a79;
}
.coupons_list_box .head_tit {
  width: 90%;
  margin: 0 auto;
  color: #da0a79;
  font-size: 32rpx;
  font-weight: bold;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-bottom: 20rpx;
}
.coupons_list_box .label {
  width: 90%;
  margin: 0 auto;
  border-radius: 20rpx;
  display: flex;
  justify-content: space-between;
  margin-bottom: 30rpx;
  background-color: #da0a7905;
  padding: 20rpx 0;
  border: 1px solid #da0a7920;
}
.coupons_list_box .label .price {
  width: 180rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #da0a79;
  font-size: 46rpx;
  font-weight: bold;
  line-height: 46rpx;
}
.coupons_list_box .label .price ._span {
  font-size: 26rpx;
  padding-left: 4rpx;
  margin-bottom: -6rpx;
}
.coupons_list_box .label .info {
  flex: 1;
  min-width: 0;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  height: 200rpx;
}
.coupons_list_box .label .info .title {
  color: #1c1c1c;
  font-size: 30rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.coupons_list_box .label .info .text {
  font-size: 28rpx;
  color: #666;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.coupons_list_box .label .info .intro {
  font-size: 26rpx;
  color: #999;
}
.coupons_list_box .label .info .btn_box {
  padding-top: 10rpx;
}
.coupons_list_box .label .info .btn_box .btn {
  width: 150rpx;
  background-color: #da0a79;
  color: #fff;
  height: 50rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 100rpx;
  font-size: 26rpx;
}
.coupons_list_box .label .button {
  width: 160rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.coupons_list_box .label .button .btn {
  background-color: #f14b5b;
  color: #fff;
  padding: 6rpx 14rpx;
  border-radius: 100rpx;
  font-size: 26rpx;
}

