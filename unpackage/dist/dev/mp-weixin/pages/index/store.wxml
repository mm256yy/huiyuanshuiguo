<view class="content"><view class="map_box"><map markers="{{markers}}" latitude="{{latitude}}" longitude="{{longitude}}"></map></view><view class="store_box"><block wx:for="{{storeList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['selectStoreTap',[index]]]]]}}" class="store" bindtap="__e"><view class="left"><image src="{{item.logo||logo}}" mode="aspectFill"></image></view><view class="right"><view class="title">{{item.title}}</view><view class="address">{{item.address}}</view></view><view class="select"><block wx:if="{{storeId===item.id}}"><aicon vue-id="{{'79017e2a-1-'+index}}" type="roundcheckfill" color="{{template.color.temp_color}}" size="25" bind:__l="__l"></aicon></block><block wx:else><aicon vue-id="{{'79017e2a-2-'+index}}" type="round" color="#eee" size="25" bind:__l="__l"></aicon></block></view></view></block></view></view>