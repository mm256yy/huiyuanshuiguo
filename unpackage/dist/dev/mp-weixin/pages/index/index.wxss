.swiper_box {
  width: 92%;
  height: 650rpx;
  position: relative;
  background-color: #fff;
  margin: 0 auto;
  border-radius: 20rpx;
  overflow: hidden;
  margin-top: 20rpx;
}
.swiper_box swiper {
  width: 100%;
  height: 100%;
  margin: 0 auto;
  overflow: hidden;
}
.swiper_box .swiper_dots {
  position: absolute;
  bottom: 30rpx;
  display: flex;
  justify-content: center;
  width: 100%;
  z-index: 6;
  left: 0;
  right: 0;
  margin: 0 auto;
}
.swiper_box .swiper_dots .dot {
  margin: 0 4rpx;
  width: 20rpx;
  height: 4rpx;
  background: #fff;
  transition: all 0.2s;
  opacity: 0.5;
}
.swiper_box .swiper_dots .active {
  opacity: 1;
}
.menu_list_box {
  display: flex;
  align-items: center;
  flex-wrap: wrap;
  width: 92%;
  margin: 0 auto;
  padding-top: 60rpx;
}
.menu_list_box .menu {
  flex-basis: 20%;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  margin-bottom: 30rpx;
}
.menu_list_box .menu .image image {
  width: 100rpx;
  height: 100rpx;
}
.menu_list_box .menu .title {
  color: #1f1f1f;
  font-size: 26rpx;
  padding-top: 10rpx;
}
.pintuan_list_box {
  width: 92%;
  margin: 0 auto;
  background-color: #F54319;
  border-radius: 20rpx;
  overflow: hidden;
  margin-top: 50rpx;
}
.pintuan_list_box .head {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 90%;
  margin: 0 auto;
  height: 120rpx;
  color: #fff;
}
.pintuan_list_box .head .left {
  font-weight: bold;
  font-size: 32rpx;
  background-image: linear-gradient(to right, #ffffff, #ffdecc);
  color: #F54319;
  border-radius: 100rpx;
  padding: 0 20rpx;
  height: 50rpx;
  display: flex;
  align-items: center;
}
.pintuan_list_box .head .right {
  color: #fff;
  font-size: 28rpx;
}
.pintuan_list_box .list_box {
  background-color: #fff;
  width: 96%;
  margin: 0 auto;
  margin-bottom: 14rpx;
  border-radius: 20rpx;
  padding-top: 30rpx;
}
.pintuan_list_box .list_box .list {
  width: 92%;
  margin: 0 auto;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding-bottom: 30rpx;
}
.pintuan_list_box .list_box .list .image {
  width: 220rpx;
}
.pintuan_list_box .list_box .list .image image {
  width: 200rpx;
  height: 200rpx;
  border-radius: 10rpx;
}
.pintuan_list_box .list_box .list .info {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  min-width: 0;
  height: 190rpx;
}
.pintuan_list_box .list_box .list .info .title {
  color: #1f1f1f;
  font-size: 28rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.pintuan_list_box .list_box .list .info .tag ._span {
  background-image: linear-gradient(to right, #F54319, #FF6D20);
  color: #fff;
  font-size: 24rpx;
  border-radius: 6rpx;
  padding: 0 6rpx;
  height: 40rpx;
}
.pintuan_list_box .list_box .list .info .times .time {
  display: flex;
  align-items: center;
}
.pintuan_list_box .list_box .list .info .times .time .line {
  background-color: #FF6D20;
  color: #fff;
  height: 40rpx;
  font-size: 24rpx;
  display: flex;
  align-items: center;
  width: 230rpx;
  justify-content: center;
  border-top-left-radius: 8rpx;
  border-bottom-left-radius: 8rpx;
}
.pintuan_list_box .list_box .list .info .times .time .txt {
  border: 1px solid #FF6D20;
  color: #FF6D20;
  height: 36rpx;
  font-size: 24rpx;
  padding: 0 10rpx;
  border-top-right-radius: 8rpx;
  border-bottom-right-radius: 8rpx;
}
.pintuan_list_box .list_box .list .info .box {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.pintuan_list_box .list_box .list .info .box .price {
  color: #F54319;
  font-size: 38rpx;
}
.pintuan_list_box .list_box .list .info .box .price ._span {
  color: #999;
  font-size: 26rpx;
  margin-left: 10rpx;
  text-decoration: line-through;
}
.pintuan_list_box .list_box .list .info .box .buy {
  background-color: #F54319;
  color: #fff;
  padding: 0 20rpx;
  border-radius: 100rpx;
  height: 46rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 26rpx;
}
.all_list_box {
  width: 100%;
  padding-top: 30rpx;
}
.all_list_box .head_box {
  width: 100%;
  height: 100rpx;
  white-space: nowrap;
  background-color: #fff;
  position: -webkit-sticky;
  position: sticky;
  left: 0;
  top: 0;
  z-index: 100;
}
.all_list_box .head_box .list {
  display: inline-block;
  line-height: 100rpx;
  color: #1f1f1f;
  font-size: 30rpx;
  padding-left: 40rpx;
  padding-right: 20rpx;
}
.all_list_box .head_box .list ._span {
  color: #1f1f1f;
  border-bottom: 3px solid #fff;
  height: 86rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.all_list_box .head_box .alabel ._span {
  color: #F54319;
  border-color: #F54319;
}
.all_list_box .goods_list {
  width: 92%;
  padding-top: 40rpx;
  margin: 0 auto;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.all_list_box .goods_list .goods {
  flex-basis: 48%;
  margin-bottom: 30rpx;
}
.all_list_box .goods_list .goods .image image {
  width: 100%;
  height: 280rpx;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
}
.all_list_box .goods_list .goods .info {
  border: 1px solid #efefef;
  border-top: none;
  border-bottom-left-radius: 20rpx;
  border-bottom-right-radius: 20rpx;
  padding: 20rpx 0;
  min-width: 0;
}
.all_list_box .goods_list .goods .info .title {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  color: #1f1f1f;
  font-size: 28rpx;
  width: 90%;
  margin: 0 auto;
  max-width: 300rpx;
}
.all_list_box .goods_list .goods .info .title ._span {
  background-color: #F54319;
  color: #fff;
  padding: 0 6rpx;
  border-radius: 6rpx;
  font-size: 24rpx;
  margin-right: 10rpx;
}
.all_list_box .goods_list .goods .info .box {
  width: 90%;
  margin: 0 auto;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding-top: 10rpx;
}
.all_list_box .goods_list .goods .info .box .left {
  flex: 1;
  min-width: 0;
  min-height: 82rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.all_list_box .goods_list .goods .info .box .left .price {
  color: #F54319;
  font-size: 30rpx;
}
.all_list_box .goods_list .goods .info .box .left .price ._span {
  color: #999;
  font-size: 26rpx;
  text-decoration: line-through;
  margin-left: 10rpx;
}
.all_list_box .goods_list .goods .info .box .left .vip {
  color: #1f1f1f;
  font-size: 28rpx;
  display: flex;
  align-items: center;
  padding-top: 6rpx;
}
.all_list_box .goods_list .goods .info .box .left .vip ._span {
  background-color: #1f1f1f;
  color: gold;
  font-size: 20rpx;
  padding: 0 6rpx;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
  margin-left: 10rpx;
}
.all_list_box .goods_list .goods .info .box .left .tuan ._span {
  margin-left: 0;
  border-top-left-radius: 0;
  border-top-right-radius: 0;
  border-bottom-right-radius: 0;
  padding: 2rpx 10rpx;
  border-radius: 6rpx;
  background-color: gold;
  color: #1f1f1f;
}
.all_list_box .goods_list .goods .info .box .right aicon {
  width: 56rpx;
  height: 56rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border: 1px solid #F54319;
  border-radius: 100%;
}
.coupons_ico {
  position: fixed;
  bottom: 200rpx;
  right: 30rpx;
  width: 120rpx;
  height: 120rpx;
  z-index: 100;
  -webkit-animation: bounce-down 2s linear infinite;
          animation: bounce-down 2s linear infinite;
}
.coupons_ico .number {
  position: absolute;
  top: 0;
  width: 100%;
  height: 96rpx;
  display: flex;
  align-items: flex-end;
  justify-content: center;
  color: #fff;
  font-size: 36rpx;
  font-weight: bold;
}
.coupons_ico .number ._span {
  font-weight: normal;
  font-size: 26rpx;
  margin-bottom: 6rpx;
  padding-right: 2rpx;
}
.coupons_show_box .bg {
  width: 750rpx;
  height: 750rpx;
}
.coupons_show_box .coupons_box {
  position: absolute;
  top: 0;
  width: 100%;
  height: 394rpx;
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: flex-end;
}
.coupons_show_box .coupons_box .number {
  color: #f4203f;
  font-size: 100rpx;
  font-weight: bold;
  display: flex;
  align-items: flex-start;
  line-height: 100rpx;
}
.coupons_show_box .coupons_box .number ._span {
  font-size: 50rpx;
  padding-right: 8rpx;
  margin-top: -16rpx;
}
.coupons_show_box .coupons_box .text {
  border-top: 1px dashed #efbd9b;
  color: #efbd9b;
  font-size: 22rpx;
  width: 320rpx;
  text-align: center;
  margin-top: 10rpx;
  padding-top: 10rpx;
}
.coupons_show_box .coupons_info {
  position: absolute;
  bottom: 0;
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  height: 290rpx;
}
.coupons_show_box .coupons_info .title {
  color: #fff;
  font-size: 42rpx;
  font-weight: bold;
}
.coupons_show_box .coupons_info .text {
  color: #fff;
  opacity: 0.8;
  font-size: 26rpx;
  padding: 12rpx 0;
}
.coupons_show_box .coupons_info .button {
  background-color: #fddfb6;
  width: 320rpx;
  height: 66rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #cc3111;
  font-weight: bold;
  border-radius: 100rpx;
  box-shadow: 0 10rpx 20rpx;
}
@-webkit-keyframes bounce-down {
25% {
    -webkit-transform: scale(1.07);
            transform: scale(1.07);
}
50%,
  100% {
    -webkit-transform: scale(1);
            transform: scale(1);
}
75% {
    -webkit-transform: scale(1.07);
            transform: scale(1.07);
}
}
@keyframes bounce-down {
25% {
    -webkit-transform: scale(1.07);
            transform: scale(1.07);
}
50%,
  100% {
    -webkit-transform: scale(1);
            transform: scale(1);
}
75% {
    -webkit-transform: scale(1.07);
            transform: scale(1.07);
}
}

