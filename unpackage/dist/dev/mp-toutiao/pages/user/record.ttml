<view class="content"><view class="headerBox"><view class="menu"><view data-event-opts="{{[['tap',[['selectMenuTap',[2]]]]]}}" class="{{(('label')+' '+(current==2?'active':''))}}" bindtap="__e"><text>消费记录</text></view><view data-event-opts="{{[['tap',[['selectMenuTap',[1]]]]]}}" class="{{(('label')+' '+(current==1?'active':''))}}" bindtap="__e"><text>充值记录</text></view></view><view class="header"><view class="label" style="flex:0 0 30%;">日期</view><view class="label" style="flex:0 0 25%;">{{(current===2?'消费':'充值')+"金额"}}</view><block tt:if="{{current===1}}"><view class="label" style="flex:0 0 20%;">赠送金额</view></block><view class="label" style="flex:0 0 25%;">当前余额</view><block tt:if="{{current===2}}"><view class="label" style="flex:0 0 20%;">订单号</view></block></view></view><view class="recordList"><block tt:for="{{userRecord.list}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view class="list"><view class="label" style="flex:0 0 30%;">{{item.dates}}</view><view class="label" style="flex:0 0 25%;">{{item.moneys}}</view><block tt:if="{{current===1}}"><view class="label" style="flex:0 0 20%;">{{item.gives}}</view></block><view class="label" style="flex:0 0 25%;">{{item.balance}}</view><block tt:if="{{item.orders&&current===2}}"><view data-event-opts="{{[['tap',[['copyOrderTap',['$0'],[[['userRecord.list','',index,'orders']]]]]]]}}" class="label" style="flex:0 0 20%;" bindtap="__e"><view class="text">复制</view></view></block></view></block><block tt:if="{{userRecord.list.length===0}}"><view class="none_list_box"><image src="../../static/images/none_data_icon_02.png" mode="aspectFill"></image><label class="_span">{{"暂无"+(current===2?'消费':'充值')+"记录"}}</label></view></block></view><view class="footer"><view class="label"><block tt:if="{{current===2}}"><view class="text">{{"累计余额消费：¥"+userRecord.money}}</view></block><block tt:if="{{current===1}}"><view class="text">{{"累计余额充值：¥"+userRecord.money}}</view></block></view></view></view>