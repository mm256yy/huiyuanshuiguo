<view class="content"><view class="head_box box_shadow"><block tt:for="{{orderMenu}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view data-event-opts="{{[['tap',[['selectMenuTap',['$0'],[[['orderMenu','',index,'id']]]]]]]}}" class="{{(('label')+' '+((orderActive===item.id)?'alabel':''))}}" bindtap="__e"><view class="tit" style="{{'border-color:'+(orderActive===item.id?'#F54319':'#fff')+';'}}">{{item.name}}</view></view></block></view><view class="order_list_box"><block tt:for="{{$root.l1}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view data-event-opts="{{[['tap',[['getDetailsTap',['$0'],[[['orderList','',index,'id']]]]]]]}}" class="order box_shadow" bindtap="__e"><view class="head"><view class="left">{{''+item.m0+'下单'}}<block tt:if="{{item.$orig.pintuan_order}}"><label class="tag _span">团</label></block></view><view class="right"><block tt:if="{{item.$orig.order_status===0}}"><label class="_span">待支付</label></block><block tt:else><block tt:if="{{item.$orig.order_status===1&&item.$orig.pintuan_order&&item.$orig.pintuan_status===1}}"><label class="_span">拼团中</label></block><block tt:else><block tt:if="{{item.$orig.order_status===1}}"><label class="_span">待发货</label></block><block tt:else><block tt:if="{{item.$orig.order_status===2}}"><label class="_span">待收货</label></block><block tt:else><block tt:if="{{item.$orig.order_status===3}}"><label class="_span">已完成</label></block><block tt:else><block tt:if="{{item.$orig.order_status===4&&!item.$orig.paypal_type}}"><label class="_span">已过期</label></block><block tt:else><block tt:if="{{item.$orig.order_status===4&&item.$orig.paypal_type}}"><label class="_span">已关闭</label></block></block></block></block></block></block></block></view></view><view class="goods_box"><block tt:for="{{item.l0}}" tt:for-item="itm" tt:for-index="idx" tt:key="idx"><view class="goods"><view class="left"><image src="{{itm.$orig.goods_pic}}" mode="aspectFill"></image></view><view class="right"><view class="title">{{itm.$orig.goods_title}}</view><view class="text">{{itm.m1}}</view><view class="box"><view class="num">{{itm.$orig.goods_price}}<label class="_span">元</label></view><view class="buy">{{"x"+itm.$orig.buy_num}}</view></view></view></view></block></view><view class="foot"><view class="left">实际支付：<label style="{{'color:'+('#F54319')+';'}}" class="_span">{{item.$orig.total_money+"元"}}</label></view><view class="right"><block tt:if="{{item.$orig.order_status===0}}"><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" data-event-params="{{({item:item.$orig})}}" class="button" style="{{'background-color:'+('#F54319')+';'+('border-color:'+('#F54319')+';')+('color:'+('#fff')+';')}}" catchtap="__e"><label class="_span">立即支付</label><u-count-down vue-id="{{'4393f126-1-'+index}}" time="{{item.m2}}" format="mm:ss" bind:__l="__l"></u-count-down></view></block><block tt:else><block tt:if="{{item.$orig.order_status===1}}"><view class="button">联系客服<button open-type="contact"></button></view></block><block tt:else><block tt:if="{{item.$orig.order_status===2}}"><view data-event-opts="{{[['tap',[['confirmOrder',['$0'],[[['orderList','',index,'id']]]]]]]}}" class="button" style="{{'background-color:'+('#F54319')+';'}}" catchtap="__e">确认收货</view></block><block tt:else><block tt:if="{{item.$orig.order_status===4}}"><view data-event-opts="{{[['tap',[['getOrderDel',[index]]]]]}}" class="button" catchtap="__e">删除订单</view></block></block></block></block></view></view></view></block><block tt:if="{{orderList.length===0}}"><view class="none_list_box"><image src="../../static/images/none_data_icon_02.png" mode="widthFix"></image><label class="_span">暂无相关订单</label></view></block></view><view class="seat_box"><view class="seat"></view></view><paypal-type vue-id="4393f126-2" paypalShow="{{paypalShow}}" money="{{userData.moneys}}" data-event-opts="{{[['^close',[['e1']]],['^paypal',[['getPaypalOrder']]]]}}" bind:close="__e" bind:paypal="__e" bind:__l="__l"></paypal-type></view>