<view class="content"><block tt:if="{{brandInfo}}"><block><view class="headerBox" style="{{('background-color:'+brandInfo.mini_color)}}"><view class="logo"><block tt:if="{{brandInfo.mini_logo}}"><image src="{{brandInfo.mini_logo}}"></image></block><block tt:if="{{brandInfo.mini_title}}"><view class="title">{{brandInfo.mini_title}}</view></block></view><block tt:if="{{brandInfo.mini_intro}}"><view class="text" style="{{('color:'+brandInfo.mini_color)}}"><text></text><label class="_span">{{brandInfo.mini_intro}}</label></view></block></view><view class="infoBox"><view class="title">开通&定制专属小程序</view><view class="text">请联系您的专属客服</view><block tt:if="{{brandInfo.mini_phone}}"><view data-event-opts="{{[['tap',[['getPhoneTap',['$0'],['brandInfo.mini_phone']]]]]}}" class="number" style="{{('color:'+brandInfo.mini_color+';border-color:'+brandInfo.mini_color)}}" bindtap="__e">{{brandInfo.mini_phone}}</view></block></view><view class="qrcode"><block tt:if="{{brandInfo.mini_wx_img}}"><view class="label" style="{{'margin-right:50rpx;'+('background-color:'+brandInfo.mini_color+'10;border-color:'+brandInfo.mini_color+'60;')}}"><image lazy-load="{{true}}" src="{{qrcodeList[0]}}" data-event-opts="{{[['tap',[['previewImage',[0]]]]]}}" bindtap="__e"></image><view class="text">{{brandInfo.mini_wx_title}}</view></view></block><block tt:if="{{brandInfo.mini_kf_img}}"><view class="label" style="{{('background-color:'+brandInfo.mini_color+'10;border-color:'+brandInfo.mini_color+'60;')}}"><image lazy-load="{{true}}" src="{{qrcodeList[1]}}" data-event-opts="{{[['tap',[['previewImage',[1]]]]]}}" bindtap="__e"></image><view class="text">{{brandInfo.mini_kf_title}}</view></view></block></view><view data-event-opts="{{[['tap',[['getMiniTap']]]]}}" class="button_box" style="{{('background-color:'+brandInfo.mini_color)}}" bindtap="__e">免费定制小程序</view></block></block></view>