<view class="content"><view class="list_box"><block tt:for="{{miaoshaGoodsList}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view data-event-opts="{{[['tap',[['getPathTap',['/pages/index/details?id='+item.id]]]]]}}" class="list" bindtap="__e"><view class="image"><image src="{{item.pic}}" mode="aspectFill"></image></view><view class="info"><view class="title">{{item.title}}</view><view class="times"><u-count-down vue-id="{{'03d14d2b-1-'+index}}" time="{{item.time_stamp}}" format="DD:HH:mm:ss" autoStart="{{true}}" millisecond="{{true}}" data-event-opts="{{[['^change',[['onChange']]]]}}" bind:change="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="time"><view class="line"><text>{{timeData.days+"天"}}</text><text>{{(timeData.hours>10?timeData.hours:'0'+timeData.hours)+"时"}}</text><text>{{timeData.minutes+"分"}}</text><text>{{timeData.seconds+"秒"}}</text></view><view class="txt">下架</view></view></u-count-down></view><view class="box"><view class="price">{{"¥"+item.mini_price}}<block tt:if="{{item.max_price}}"><label class="_span">{{"¥"+item.max_price}}</label></block></view><view class="buy">去抢购</view></view></view></view></block><block tt:if="{{miaoshaGoodsList.length===0}}"><view class="none_list_box"><image src="../../static/images/none_data_icon_01.png" mode="widthFix"></image><label class="_span">暂无秒杀商品</label></view></block></view></view>