<view class="content"><view class="map_box"><map markers="{{markers}}" latitude="{{latitude}}" longitude="{{longitude}}"></map></view><view class="store_box"><block tt:for="{{storeList}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view data-event-opts="{{[['tap',[['selectStoreTap',[index]]]]]}}" class="store" bindtap="__e"><view class="left"><image src="{{item.logo||logo}}" mode="aspectFill"></image></view><view class="right"><view class="title">{{item.title}}</view><view class="address">{{item.address}}</view></view><view class="select"><block tt:if="{{storeId===item.id}}"><aicon vue-id="{{'7904566c-1-'+index}}" type="roundcheckfill" color="{{template.color.temp_color}}" size="25" bind:__l="__l"></aicon></block><block tt:else><aicon vue-id="{{'7904566c-2-'+index}}" type="round" color="#eee" size="25" bind:__l="__l"></aicon></block></view></view></block></view></view>