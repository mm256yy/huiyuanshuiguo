<view class="bg" style="{{'background-color:'+(template.color.temp_color)+';'}}"><view class="container"><block tt:if="{{contentobj.style==1}}"><view class="frame_view"><view class="frame_row" style="margin-top:48rpx;"><view class="{{(('frame_item')+' '+('item'+color[0]))}}"><image src="{{award[0].pic}}"></image><text>{{award[0].title}}</text></view><view class="{{(('frame_item')+' '+('item'+color[1]))}}"><image src="{{award[1].pic}}"></image><text>{{award[1].title}}</text></view><view class="{{(('frame_item')+' '+('item'+color[2]))}}"><image src="{{award[2].pic}}"></image><text>{{award[2].title}}</text></view></view><view class="frame_row"><view class="{{(('frame_item')+' '+('item'+color[7]))}}"><image src="{{award[7].pic}}"></image><text>{{award[7].title}}</text></view><view style="text-align:center;color:#fff;"><view style="font-size:26rpx;">{{contentobj.score+"积分/次"}}</view><view style="font-size:26rpx;padding-top:10rpx;">{{"可用"+(userInfoData.score||0)+"积分"}}</view></view><view class="{{(('frame_item')+' '+('item'+color[3]))}}"><image src="{{award[3].pic}}"></image><text>{{award[3].title}}</text></view></view><view class="frame_row"><view class="{{(('frame_item')+' '+('item'+color[6]))}}"><image src="{{award[6].pic}}"></image><text>{{award[6].title}}</text></view><view class="{{(('frame_item')+' '+('item'+color[5]))}}"><image src="{{award[5].pic}}"></image><text>{{award[5].title}}</text></view><view class="{{(('frame_item')+' '+('item'+color[4]))}}"><image src="{{award[4].pic}}"></image><text>{{award[4].title}}</text></view></view></view></block><block tt:if="{{contentobj.style==1}}"><view class="luckbtulist"><view data-event-opts="{{[['tap',[['clickLuckTap']]]]}}" class="luckone" bindtap="__e"><view style="margin-top:30rpx;" class="_p">{{(contentobj.score||0)+"积分抽一次"}}</view></view></view></block><block tt:if="{{contentobj.style==2}}"><view class="lucky-draw" style="{{(scale)}}"><image class="lucky-draw__bg" style="width:750rpx;" src="http://cdn.sncat.ddyy.top/ece3f08cadefa73af31bcf04a503e6fe.png" mode="widthFix"></image><view class="lucky-draw__objects" style="{{(rotateStyle)}}"><image class="lucky-draw__objects__bg" src="http://cdn.sncat.ddyy.top/a231e3f694f0d423607325bda01de841.png"></image><block tt:for="{{award}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view class="lucky-draw__objects__object" style="{{'transform:'+('translate(-50%) rotate('+360/8*index+'deg)')+';'}}"><view class="lucky-draw__objects__object__title">{{item.title}}</view><image class="lucky-draw__objects__object__icon" src="{{item.pic}}"></image></view></block></view><image class="lucky-draw__start" src="http://cdn.sncat.ddyy.top/0dd505a71bce00baf6f952cbfa50deab.png" mode="widthFix" data-event-opts="{{[['tap',[['startaTap']]]]}}" bindtap="__e"></image><view style="color:#fff;display:flex;justify-content:center;align-items:center;font-size:32rpx;"><text style="color:gold;padding:0 10rpx;">{{contentobj.score}}</text>积分/次，可用<text style="color:gold;padding:0 10rpx;">{{userInfoData.score||0}}</text>积分</view></view></block><view class="lucktips"><view class="lucktab"><view data-event-opts="{{[['tap',[['toggleTabs',[0]]]]]}}" class="{{(('luckrule')+' '+(nowIndex==0?'action':''))}}" bindtap="__e">活动规则</view><view data-event-opts="{{[['tap',[['toggleTabs',[1]]]]]}}" class="{{(('record')+' '+(nowIndex==1?'action':''))}}" bindtap="__e">获奖记录</view></view><view hidden="{{!(nowIndex==0)}}" class="ruleinof"><view class="ruleconten" style="width:100%;height:100%;margin-top:40rpx;overflow-y:auto;"><rich-text nodes="{{content}}"></rich-text></view></view><view hidden="{{!(nowIndex==1)}}" class="ruleinof"><block tt:if="{{userlist.length===0}}"><view class="none_list_box" style="height:45vh;"><image src="../../static/images/none_data_icon_01.png"></image><label class="_span">暂无获奖记录</label></view></block><block tt:else><view style="width:100%;height:496rpx;margin-top:40rpx;overflow-y:auto;"><block tt:for="{{userlist}}" tt:for-item="item" tt:for-index="inde