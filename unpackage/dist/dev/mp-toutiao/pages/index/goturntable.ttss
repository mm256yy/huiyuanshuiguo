page {
  width: 750rpx;
  overflow-x: hidden;
}
.lucky-draw {
  position: relative;
  width: -webkit-fit-content;
  width: fit-content;
}
.lucky-draw__bg {
  width: 686rpx;
  display: block;
}
.lucky-draw__lights {
  position: absolute !important;
  width: 676rpx;
  left: 6rpx;
  top: 10rpx;
  display: block;
  -webkit-animation: infinite 1s alternate lights steps(1, end);
          animation: infinite 1s alternate lights steps(1, end);
}
.lucky-draw__objects {
  position: absolute;
  top: 100rpx;
  left: 135rpx;
  right: 63rpx;
  bottom: 78rpx;
  width: 500rpx;
  height: 500rpx;
}
.lucky-draw__objects__bg {
  position: absolute;
  width: 500rpx;
  height: 500rpx;
}
.lucky-draw__objects__object {
  position: absolute;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  top: 0;
  -webkit-transform-origin: 50% 250rpx;
          transform-origin: 50% 250rpx;
}
.lucky-draw__objects__object__title {
  margin-top: 60rpx;
  color: #F54319;
  font-size: 30rpx;
  -webkit-transform: scale(0.7);
}
.lucky-draw__objects__object__icon {
  width: 52rpx;
  height: 52rpx;
  display: block;
  margin: 0 auto 0;
}
.lucky-draw__start {
  width: 150rpx;
  height: 170rpx;
  display: block;
  position: absolute;
  left: 310rpx;
  top: 242rpx;
}
@-webkit-keyframes lights {
0% {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
50% {
    -webkit-transform: rotate(15deg);
            transform: rotate(15deg);
}
100% {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
}
@keyframes lights {
0% {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
50% {
    -webkit-transform: rotate(15deg);
            transform: rotate(15deg);
}
100% {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
}
