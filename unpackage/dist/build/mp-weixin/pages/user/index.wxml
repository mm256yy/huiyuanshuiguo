<view class="content"><view class="{{['user_bg_box',(!userInfoData)?'none_user_bg_box':'']}}"><block wx:if="{{userInfoData}}"><view class="user_box"><view class="avatar"><image src="{{userInfoData.avatar_url}}" mode="aspectFill"></image><view class="edit"><aicon vue-id="5c2bbbc2-1" type="camerafill" size="14" color="#fff" bind:__l="__l"></aicon></view><button open-type="chooseAvatar" data-event-opts="{{[['chooseavatar',[['chooseavatar',['$event']]]]]}}" bindchooseavatar="__e"></button></view><view class="info"><view class="name"><block wx:if="{{!editNameStatus}}"><block><view class="tit">{{userNickName}}</view><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="edit" bindtap="__e"><aicon vue-id="5c2bbbc2-2" type="post" size="18" color="#fff" bind:__l="__l"></aicon></view></block></block><block wx:else><block><input type="nickname" confirm-type="send" data-event-opts="{{[['confirm',[['updatenickname',['$event']]]]]}}" value="{{userNickName}}" bindconfirm="__e"/></block></block></view><view class="text">{{"UID:"+userInfoData.id}}</view></view></view></block><block wx:else><view data-event-opts="{{[['tap',[['getPathTap',['/pages/user/login']]]]]}}" class="user_box" bindtap="__e"><view class="avatar"><image src="../../static/images/avatar.png" mode="aspectFill"></image></view><view class="info"><view class="name"><view class="tit">{{userNickName}}</view></view><view class="text">您还未登录哦～</view></view></view></block><view class="money_box"><view data-event-opts="{{[['tap',[['getPathTap',['/pages/user/money']]]]]}}" class="label" bindtap="__e"><view class="number money_num"><label class="_span">¥</label>{{userInfoData.moneys||0}}</view><view class="name">余额</view></view><view data-event-opts="{{[['tap',[['getPathTap',['/pages/index/score']]]]]}}" class="label" bindtap="__e"><view class="number">{{userInfoData.score||0}}<label class="_span">分</label></view><view class="name">积分</view></view><view data-event-opts="{{[['tap',[['getPathTap',['/pages/user/coupons']]]]]}}" class="label" bindtap="__e"><view class="number">{{couponsNumber}}<label class="_span">张</label></view><view class="name">优惠券</view></view></view><block wx:if="{{userInfoData}}"><view class="vip_box"><view data-event-opts="{{[['tap',[['getPathTap',['/pages/user/member']]]]]}}" class="vip" bindtap="__e"><view class="left"><aicon vue-id="5c2bbbc2-3" type="crownfill" size="18" color="#F54319" bind:__l="__l"></aicon><block wx:if="{{userInfoData.level_id}}"><label class="_span">{{"尊贵的"+userInfoData.level_title+"，购物可享"+userInfoData.discount+"折优惠"}}</label></block><block wx:else><label class="_span">您还不是会员，升级会员可享优惠</label></block></view><view class="right"><aicon vue-id="5c2bbbc2-4" type="unfold-copy" size="16" color="#F54319" bind:__l="__l"></aicon></view></view></view></block></view><view class="order_list_box"><block wx:for="{{orderList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['getPathTap',['$0'],[[['orderList','',index,'path']]]]]]]}}" class="label" bindtap="__e"><view class="image"><aicon vue-id="{{'5c2bbbc2-5-'+index}}" type="{{item.icon}}" size="25" color="#F54319" bind:__l="__l"></aicon></view><view class="text">{{item.name}}</view></view></block></view><block wx:if="{{myBannerData}}"><view class="banner_box"><view class="pic"><image src="{{myBannerData.pic}}" mode="widthFix" data-event-opts="{{[['tap',[['getPathTap',['$0'],['myBannerData.target_data']]]]]}}" bindtap="__e"></image></view></view></block><view class="menu_box"><block wx:for="{{menuList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['getPathTap',['$0'],[[['menuList','',index,'path']]]]]]]}}" class="label" bindtap="__e"><view class="icons"><aicon vue-id="{{'5c2bbbc2-6-'+index}}" type="{{item.icon}}" size="22" color="#1c1c1c" bind:__l="__l"></aicon></view><view class="name">{{item.name}}</view><view class="right"><aicon vue-id="{{'5c2bbbc2-7-'+index}}" type="unfold-copy" size="20" color="#bbb" bind:__l="__l"></aicon></view><block wx:if="{{item.id===4}}"><button open-type="contact"></button></block></view></block></view><view class="power_by"><about vue-id="5c2bbbc2-8" bind:__l="__l"></about></view><tabbar vue-id="5c2bbbc2-9" current="{{3}}" cart="{{cartNums}}" bind:__l="__l"></tabbar></view>